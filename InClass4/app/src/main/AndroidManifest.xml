<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mad.group12.passwordgenerator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/password_generator"
        android:label="@string/app_name"
        android:roundIcon="@drawable/password_generator"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PasswordDashboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PasswordGenerator"></activity>
    </application>

</manifest>